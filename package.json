{
  "name": "@arayui/rainy-day",
  "type": "module",
  "version": "1.0.2",
  "description": "下雨天气背景插件",
  "author": "WuCheng-cn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WuCheng-cn/AnyRainyDay"
  },
  "keywords": [
    "rainy-day",
    "typescript",
    "plugin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "cz": "cz",
    "release": "pnpm build && release-it",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^24.3.0",
    "@types/three": "^0.175.0",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "commitlint-config-cz": "^0.13.3",
    "commitlint-config-gitmoji": "^2.3.1",
    "conventional-changelog": "^7.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.5.0",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "release-it": "^19.0.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.0.0",
    "vitepress": "2.0.0-alpha.11",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
  "lint-staged": {
    "src/**/*.{?([cm])[jt]s?(x),vue,md,json}": [
      "eslint --cache --fix"
    ]
  }
}
